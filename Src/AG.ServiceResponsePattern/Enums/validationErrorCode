namespace AG.ServiceResponsePattern.Enums
{
    /// <summary>Defines a list of common validation error codes</summary>
    public enum ValidationErrorCode
    {
        NoError          = 0,
        Mandatory        = 1,
        BelowMinLength   = 2,
        AboveMaxLength   = 3,
        BelowMinValue    = 2,
        AboveMaxValue    = 3
    }
}
